{
  "name": "pegasus-elite-hub",
  "version": "1.2.0",
  "private": true,
  "description": "Production-ready management system with Express and Supabase",
  "scripts": {
    "dev": "vite --port 8080",
    "build": "vite build",
    "build:check": "tsc && vite build",
    "preview": "vite preview --port 8080",
    "start": "bun server.js",
    "start:backend": "bun backend.js",
    "start:all": "concurrently \"bun run dev\" \"bun run start:backend\"",
    "deploy:vercel": "vercel --prod",
    "deploy:build": "bun run build"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.90.18",
    "@tanstack/react-query-devtools": "^5.91.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.21.1",
    "firebase-admin": "^13.0.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.453.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.6.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.1.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}